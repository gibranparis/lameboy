    "three": "^0.180.0"
  },
  "devDependencies": {
    "@types/node": "24.10.1",
    "eslint": "9.9.0",
    "eslint-config-next": "15.5.0",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "tailwindcss": "^4.1.14"
  }
}
